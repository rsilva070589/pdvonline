{
  "name": "vue3-nouislider",
  "description": "A Vue3 wrapper for noUiSlider",
  "version": "1.0.0",
  "author": {
    "name": "Sitronik",
    "email": "sitronik92@mail.ru"
  },
  "keywords": [
    "vue",
    "vuejs",
    "plugin"
  ],
  "license": "MIT",
  "main": "dist/vue-nouislider.common.js",
  "unpkg": "dist/vue-nouislider.browser.js",
  "module": "src/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --config config/webpack.config.dev.js --progress --watch",
    "build": "npm run build:browser && npm run build:common",
    "build:browser": "cross-env NODE_ENV=production webpack --config config/webpack.config.browser.js --progress --hide-modules",
    "build:common": "cross-env NODE_ENV=production webpack --config config/webpack.config.common.js --progress --hide-modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sitronik/vue3-nouislider.git"
  },
  "bugs": {
    "url": "https://github.com/sitronik/vue3-nouislider/issues"
  },
  "homepage": "https://github.com/sitronik/vue3-nouislider#readme",
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-env": "^1.2.1",
    "babel-preset-stage-2": "^6.22.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "eslint": "^6.5.1",
    "eslint-config-walmart": "^2.2.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-vue": "^6.2.2",
    "extract-text-webpack-plugin": "^2.0.0",
    "node-sass": "^4.5.2",
    "sass-loader": "^6.0.3",
    "vue-loader": "^11.1.4",
    "vue-template-compiler": "^2.2.6",
    "vuemit": "1.0.3",
    "webpack": "^2.2.1",
    "webpack-merge": "^2.6.1"
  },
  "dependencies": {
    "nouislider": "9.2.0"
  }
}

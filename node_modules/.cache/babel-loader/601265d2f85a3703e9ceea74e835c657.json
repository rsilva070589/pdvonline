onst noui_update2 = data => {\n      if (data) {\n        noui_min2.value = data[0];\n        noui_max2.value = data[1];\n        noui_min2_perc.value = parseFloat((noui_min2.value * 0.01).toFixed(2));\n        noui_max2_perc.value = parseFloat((noui_max2.value * 0.01).toFixed(2));\n      }\n    };\n\n    const noui_update3 = data => {\n      if (data) {\n        noui_min3.value = parseFloat(data[0]);\n        noui_slide3();\n      }\n    };\n\n    const noui_update4 = data => {\n      if (data) {\n        noui_min4.value = parseFloat(data[0]);\n      }\n    };\n\n    const set_locked_values = () => {\n      locked_values.value = [Number(noui_min3.value), Number(noui_min4.value)];\n    };\n\n    const noui_slide3 = () => {\n      if (is_lock.value) {\n        let val = JSON.parse(JSON.stringify(noui_min3.value));\n        val -= locked_values.value[0] - locked_values.value[1];\n        noui_min4.value = parseFloat(val.toFixed(2));\n      }\n    };\n\n    const __returned__ = {\n      code_arr,\n      date1,\n      date2,\n      date3,\n      date4,\n      slider1,\n      slider2,\n      slider3,\n      slider4,\n      noui_min1,\n      noui_max1,\n      noui_min2,\n      noui_max2,\n      noui_min2_perc,\n      noui_max2_perc,\n      noui_min3,\n      noui_min4,\n      is_lock,\n      locked_values,\n      toggleCode,\n      noui_change1,\n      noui_update1,\n      noui_update2,\n      noui_update3,\n      noui_update4,\n      set_locked_values,\n      noui_slide3,\n      onMounted,\n      ref,\n      highlight,\n\n      get flatPickr() {\n        return flatPickr;\n      },\n\n      get useMeta() {\n        return useMeta;\n      }\n\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n\n};","map":{"version":3,"mappings":";AA0pBI,SAASA,SAAT,EAAoBC,GAApB,QAA+B,KAA/B;AAEA,OAAO,iCAAP;AACA,OAAOC,SAAP,MAAsB,oCAAtB,C,CAEA;;AACA,OAAO,0CAAP,C,CAEA;;AACA,OAAOC,SAAP,MAAsB,
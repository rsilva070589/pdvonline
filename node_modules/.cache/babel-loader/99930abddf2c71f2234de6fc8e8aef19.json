{"ast":null,"code":"import { onMounted, ref } from 'vue';\nimport '@/assets/sass/apps/notes.scss';\nimport { useMeta } from '@/composables/use-meta';\nexport default {\n  __name: 'notes',\n\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n\n    useMeta({\n      title: 'Notes'\n    });\n    let notesMailModal = ref(null);\n    const params = ref({\n      id: null,\n      title: '',\n      description: ''\n    });\n    const is_show_note_menu = ref(false);\n    const notes_list = ref([]);\n    const filterd_notes_list = ref('');\n    const selected_tab = ref('all');\n    onMounted(() => {\n      initPopup();\n      bind_notes();\n    });\n\n    const initPopup = () => {\n      notesMailModal = new window.bootstrap.Modal(document.getElementById('notesMailModal'));\n    };\n\n    const bind_notes = () => {\n      notes_list.value = [{\n        id: 1,\n        title: 'Meeting with Kelly',\n        description: 'Curabitur facilisis vel elit sed dapibus sodales purus rhoncus.',\n        date: '11/01/2020',\n        is_fav: false,\n        tag: 'personal'\n      }, {\n        id: 2,\n        title: 'Receive Package',\n        description: 'Facilisis curabitur facilisis vel elit sed dapibus sodales purus.',\n        date: '11/02/2020',\n        is_fav: true,\n        tag: ''\n      }, {\n        id: 3,\n        title: 'Download Docs',\n        description: 'Proin a dui malesuada, laoreet mi vel, imperdiet diam quam laoreet.',\n        date: '11/04/2020',\n        is_fav: false,\n        tag: 'work'\n      }, {\n        id: 4,\n        title: 'Meeting at 4:50pm',\n        description: 'Excepteur sint occaecat cupidatat non proident, anim id est laborum.',\n        date: '11/08/2020',\n        is_fav: false,\n        tag: ''\n      }, {\n        id: 5,\n        title: 'Backup Files EOD',\n        description: 'Maecenas condimentum neque mollis, egestas leo ut, gravida.',\n        date: '11/09/2020',\n        is_fav: false,\n        tag: ''\n      }, {\n        id: 6,\n        title: 'Download Server Logs',\n        description: 'Suspendisse efficitur diam quis gravida. Nunc molestie est eros.',\n        date: '11/09/2020',\n        is_fav: false,\n        tag: 'social'\n      }, {\n        id: 7,\n        title: 'Team meet at Starbucks',\n        description: 'Etiam a odio eget enim aliquet laoreet lobortis sed ornare nibh.',\n        date: '11/10/2020',\n        is_fav: false,\n        tag: ''\n      }, {\n        id: 8,\n        title: 'Create new users Profile',\n        description: 'Duis aute irure in nulla pariatur. Etiam a odio eget enim aliquet.',\n        date: '11/11/2020',\n        is_fav: false,\n        tag: 'important'\n      }];\n      search_notes();\n    };\n\n    const search_notes = () => {\n      if (selected_tab.value != 'fav') {\n        if (selected_tab.value != 'all') {\n          filterd_notes_list.value = notes_list.value.filter(d => d.tag == selected_tab.value);\n        } else {\n          filterd_notes_list.value = notes_list.value;\n        }\n      } else {\n        filterd_notes_list.value = notes_list.value.filter(d => d.is_fav);\n      }\n    };\n\n    const note_class = note => {\n      let cls = '';\n\n      if (note.tag) {\n        cls = 'note-' + note.tag;\n      }\n\n      if (note.is_fav) {\n        cls += ' note-fav';\n      }\n\n      return cls;\n    };\n\n    const tab_changed = type => {\n      selected_tab.value = type;\n      search_notes();\n      is_show_note_menu.value = false;\n    };\n\n    const set_fav = note => {\n      note.is_fav = !note.is_fav;\n    };\n\n    const set_tag = (note, name) => {\n      note.tag = name;\n      search_notes();\n    };\n\n    const edit_note = note => {\n      params.value = {\n        id: null,\n        title: '',\n        description: ''\n      };\n\n      if (note) {\n        params.value = JSON.parse(JSON.stringify(note));\n      }\n\n      notesMailModal.show();\n    };\n\n    const save_note = () => {\n      if (!params.value.title) {\n        showMessage('Title is required.', 'error');\n        return false;\n      } //add note\n\n\n      let max_note_id = notes_list.value.reduce((max, character) => character.id > max ? character.id : max, notes_list.value[0].id);\n\n      if (!max_note_id) {\n        max_note_id = 0;\n      }\n\n      let dt = new Date();\n      let note = {\n        id: max_note_id + 1,\n        title: params.value.title,\n        description: params.value.description,\n        date: dt.getDate() + '/' + Number(dt.getMonth()) + 1 + '/' + dt.getFullYear(),\n        is_fav: false,\n        tag: ''\n      };\n      notes_list.value.splice(0, 0, note);\n      search_notes();\n      showMessage('Note saved successfully.');\n      notesMailModal.hide();\n    };\n\n    const delete_note = note => {\n      notes_list.value = notes_list.value.filter(d => d.id != note.id);\n      search_notes();\n      showMessage('Note deleted successfully.');\n    };\n\n    const showMessage = (msg = '', type = 'success') => {\n      const toast = window.Swal.mixin({\n        toast: true,\n        position: 'top',\n        showConfirmButton: false,\n        timer: 3000\n      });\n      toast.fire({\n        icon: type,\n        title: msg,\n        padding: '10px 20px'\n      });\n    };\n\n    const __returned__ = {\n      get notesMailModal() {\n        return notesMailModal;\n      },\n\n      set notesMailModal(v) {\n        notesMailModal = v;\n      },\n\n      params,\n      is_show_note_menu,\n      notes_list,\n      filterd_notes_list,\n      selected_tab,\n      initPopup,\n      bind_notes,\n      search_notes,\n      note_class,\n      tab_changed,\n      set_fav,\n      set_tag,\n      edit_note,\n      save_note,\n      delete_note,\n      showMessage,\n      onMounted,\n      ref,\n\n      get useMeta() {\n        return useMeta;\n      }\n\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n\n};","map":{"version":3,"mappings":"AAkSI,SAASA,SAAT,EAAoBC,GAApB,QAA+B,KAA/B;AACA,OAAO,+BAAP;AAEA,SAASC,OAAT,QAAwB,wBAAxB;;;;;;;;;IACAA,OAAO,CAAC;MAAEC,KAAK,EAAE;IAAT,CAAD,CAAP;IAEA,IAAIC,cAAc,GAAGH,GAAG,CAAC,IAAD,CAAxB;IACA,MAAMI,MAAM,GAAGJ,GAAG,CAAC;MAAEK,EAAE,EAAE,IAAN;MAAYH,KAAK,EAAE,EAAnB;MAAuBI,WAAW,EAAE;IAApC,CAAD,CAAlB;IACA,MAAMC,iBAAiB,GAAGP,GAAG,CAAC,KAAD,CAA7B;IACA,MAAMQ,UAAU,GAAGR,GAAG,CAAC,EAAD,CAAtB;IACA,MAAMS,kBAAkB,GAAGT,GAAG,CAAC,EAAD,CAA9B;IACA,MAAMU,YAAY,GAAGV,GAAG,CAAC,KAAD,CAAxB;IAEAD,SAAS,CAAC,MAAM;MACZY,SAAS;MACTC,UAAU;IACb,CAHQ,CAAT;;IAKA,MAAMD,SAAS,GAAG,MAAM;MACpBR,cAAc,GAAG,IAAIU,MAAM,CAACC,SAAP,CAAiBC,KAArB,CAA2BC,QAAQ,CAACC,cAAT,CAAwB,gBAAxB,CAA3B,CAAjB;IACH,CAFD;;IAIA,MAAML,UAAU,GAAG,MAAM;MACrBJ,UAAU,CAACU,KAAX,GAAmB,CACf;QAAEb,EAAE,EAAE,CAAN;QAASH,KAAK,EAAE,oBAAhB;QAAsCI,WAAW,EAAE,iEAAnD;QAAsHa,IAAI,EAAE,YAA5H;QAA0IC,MAAM,EAAE,KAAlJ;QAAyJC,GAAG,EAAE;MAA9J,CADe,EAEf;QAAEhB,EAAE,EAAE,CAAN;QAASH,KAAK,EAAE,iBAAhB;QAAmCI,WAAW,EAAE,mEAAhD;QAAqHa,IAAI,EAAE,YAA3H;QAAyIC,MAAM,EAAE,IAAjJ;QAAuJC,GAAG,EAAE;MAA5J,CAFe,EAGf;QAAEhB,EAAE,EAAE,CAAN;QAASH,KAAK,EAAE,eAAhB;QAAiCI,WAAW,EAAE,qEAA9C;QAAqHa,IAAI,EAAE,YAA3H;QAAyIC,MAAM,EAAE,KAAjJ;QAAwJC,GAAG,EAAE;MAA7J,CAHe,EAIf;QAAEhB,EAAE,EAAE,CAAN;QAASH,KAAK,EAAE,mBAAhB;QAAqCI,WAAW,EAAE,sEAAlD;QAA0Ha,IAAI,EAAE,YAAhI;QAA8IC,MAAM,EAAE,KAAtJ;QAA6JC,GAAG,EAAE;MAAlK,CAJe,EAKf;QAAEhB,EAAE,EAAE,CAAN;QAASH,KAAK,EAAE,kBAAhB;QAAoCI,WAAW,EAAE,6DAAjD;QAAgHa,IAAI,EAAE,YAAtH;QAAoIC,MAAM,EAAE,KAA5I;QAAmJC,GAAG,EAAE;MAAxJ,CALe,EAMf;QAAEhB,EAAE,EAAE,CAAN;QAASH,KAAK,EAAE,sBAAhB;QAAwCI,WAAW,EAAE,kEAArD;QAAyHa,IAAI,EAAE,YAA/H;QAA6IC,MAAM,EAAE,KAArJ;QAA4JC,GAAG,EAAE;MAAjK,CANe,EAOf;QAAEhB,EAAE,EAAE,CAAN;QAASH,KAAK,EAAE,wBAAhB;QAA0CI,WAAW,EAAE,kEAAvD;QAA2Ha,IAAI,EAAE,YAAjI;QAA+IC,MAAM,EAAE,KAAvJ;QAA8JC,GAAG,EAAE;MAAnK,CAPe,EAQf;QACIhB,EAAE,EAAE,CADR;QAEIH,KAAK,EAAE,0BAFX;QAGII,WAAW,EAAE,oEAHjB;QAIIa,IAAI,EAAE,YAJV;QAKIC,MAAM,EAAE,KALZ;QAMIC,GAAG,EAAE;MANT,CARe,CAAnB;MAiBAC,YAAY;IACf,CAnBD;;IAqBA,MAAMA,YAAY,GAAG